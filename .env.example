# Existing Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_anon_key

# Admin Panel Route (Change this to hide your admin panel)
# Examples: 'dashboard', 'management', 'backend', 'control-panel', 'sys-2026', etc.
# Default: 'sys-panel-2026'
VITE_ADMIN_ROUTE_PATH=sys-panel-2026

# Admin Panel Security - Add this encryption key
# Generate a strong random key: openssl rand -base64 32
VITE_ADMIN_ENCRYPTION_KEY=9f3c1a8e6d4b2a7c5e9d0f4b8a6c1e7d3f9b2c5a8e6d1f4c7b0a9e5d2c8f6

# Session Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_ADMIN_MAX_INACTIVE_TIME=1800000
